{
  "name": "donutloop/restful-api-workflow-bundle",
  "description": "Create workflows for your REST API",
  "keywords": ["api", "workflow", "rest"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Marcel Edmund Franke"
    }
  ],
  "require": {
    "php": ">=7.0",
    "symfony/symfony": "3.1.*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-cache-bundle": "^1.2",
    "symfony/monolog-bundle": "^2.8",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "jms/serializer-bundle": "^1.1",
    "friendsofsymfony/rest-bundle": "^1.7",
    "willdurand/hateoas-bundle": "^1.1",
    "white-october/pagerfanta-bundle": "^1.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0"
  },
  "autoload": {
    "psr-4": {"Donutloop\\RestfulApiWorkflowBundle\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Donutloop\\RestfulApiWorkflowBundle\\": "tests/"}
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
